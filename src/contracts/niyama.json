  "source": "// SPDX-License-Identifier: MIT\npragma solidity >=0.4.21 <0.7.0;\ncontract niyama {\n    mapping(address => string) public companies;\n    mapping(address => string) public reseller;\n    \n    function addCatalog (string memory file_hash) public {\n        //companies[msg.sender].push(file_hash);\n        companies[msg.sender]=file_hash;\n        \n    }\n    \n    function subscribe(string memory catalog) public {\n        reseller[msg.sender] = catalog;\n    }\n}",
